{
  "name": "convenia/timeliner",
  "license": "MIT",
  "description": "Make a timeline in DynamoDB, mirror your models to timnelineformat",
  "keywords": [
    "timeline",
    "mirror",
    "ModelReflection",
    "Laravel"
  ],
  "homepage": "http://github.com/convenia/timeliner",
  "authors": [
    {
      "name": "Victor Ventura",
      "email": "euventura@gmail.com"
    },
    {
      "name": "Bruno Briante"
    },
    {
      "name": "Tauan Lemos"
    },
    {
      "name": "Anderson Flores"
    }
  ],
  "support": {
    "issues": "https://github.com/convenia/timeliner/issues",
    "source": "https://github.com/convenia/timeliner"
  },
  "require": {
    "php": ">=7",
    "illuminate/support": "~5.0|5.1|5.2|~5.3|~5.4|~5.5"
  },
  "require-dev": {
    "codacy/coverage": "^1.0",
    "friendsofphp/php-cs-fixer": "^1.13",
    "mockery/mockery": "^0.9.4",
    "orchestra/testbench": "~3.0",
    "phpunit/phpunit": "^4.0|^5.0|^6.0"
  },
  "autoload": {
    "psr-4": {
      "Convenia\\Timeliner\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Convenia\\Timeliner\\Tests\\": "tests"
    }
  },
  "scripts": {
    "test": "./vendor/bin/phpunit",
    "fixer": "./vendor/bin/php-cs-fixer fix --verbose"
  },
  "config": {
    "preferred-install": "dist",
    "sort-packages": true
  }
}
